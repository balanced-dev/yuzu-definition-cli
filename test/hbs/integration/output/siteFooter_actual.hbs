<div class="site-footer{{#each _modifiers}} site-footer--{{this}}{{/each}}">
<div class="site-footer__address">
{{#if address.email.href}}
<a class="site-footer__address__email" href="{{address.email.href}}" title="{{address.email.title}}" {{#if address.email.isNewTab}}target="_blank"{{/if}} {{#if address.email.isExternalLink}}rel="noopener noreferrer"{{/if}}>
{{address.email.label}}
</a>
{{/if}}
{{#if address.telephone.href}}
<a class="site-footer__address__telephone" href="{{address.telephone.href}}" title="{{address.telephone.title}}" {{#if address.telephone.isNewTab}}target="_blank"{{/if}} {{#if address.telephone.isExternalLink}}rel="noopener noreferrer"{{/if}}>
{{address.telephone.label}}
</a>
{{/if}}
{{#if address.addressLines.[0]}}
<div class="site-footer__address__address-lines">
{{#each address.addressLines}}
{{#if line}}
<div class="site-footer__address__address-lines__line">
{{line}}
</div>
{{/if}}
{{/each}}
</div>
{{/if}}
</div>
{{#if links.[0]}}
<div class="site-footer__links">
{{#each links}}
{{#if this.href}}
<a class="site-footer__links__link" href="{{this.href}}" title="{{this.title}}" {{#if this.isNewTab}}target="_blank"{{/if}} {{#if this.isExternalLink}}rel="noopener noreferrer"{{/if}}>
{{this.label}}
</a>
{{/if}}
{{/each}}
</div>
{{/if}}
{{#if documentLinks.[0]}}
<div class="site-footer__document-links">
{{#each documentLinks}}
{{#if this.href}}
<a class="site-footer__document-links__document-link" href="{{this.href}}" title="{{this.title}}" {{#if this.isNewTab}}target="_blank"{{/if}} {{#if this.isExternalLink}}rel="noopener noreferrer"{{/if}}>
{{this.label}}
</a>
{{/if}}
{{/each}}
</div>
{{/if}}
{{#if socialLinks.[0]}}
<div class="site-footer__social-links">
{{#each socialLinks}}
{{#if this.href}}
<a class="site-footer__social-links__social-link" href="{{this.href}}" title="{{this.title}}" {{#if this.isNewTab}}target="_blank"{{/if}} {{#if this.isExternalLink}}rel="noopener noreferrer"{{/if}}>
{{this.label}}
</a>
{{/if}}
{{/each}}
</div>
{{/if}}
{{#if copyrightMessage}}
<div class="site-footer__copyright-message">
{{copyrightMessage}}
</div>
{{/if}}
{{#if form}}
<div class="site-footer__form">
{{!-- {{> parForm form }} --}}
</div>
{{/if}}
</div>